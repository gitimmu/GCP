CREATE database codethunder;
 after creating database ligin to specified database: use codethunder;
 
CREATE TABLE contacts ( sno int(11) NOT NULL AUTO_INCREMENT, name varchar(50) DEFAULT NULL, phone_num int(10) DEFAULT NULL, msg varchar(1000) DEFAULT NULL, date date DEFAULT NULL, email varchar(40) DEFAULT NULL, PRIMARY KEY (sno) ) ENGINE=InnoDB AUTO_INCREMENT=29 DEFAULT CHARSET=latin1;

